.button-style {
    width: auto;
    height: auto;
    background-color: rgba(0, 0, 0, 0.47);
    background-image: none;
    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    border-bottom-width: 0;
    border-top-left-radius: 10px;
    border-bottom-left-radius: 10px;
    border-top-right-radius: 10px;
    border-bottom-right-radius: 10px;
    font-size: 40px;
    -unity-font: url('project://database/Assets/RDTS/Scenes/DemoScenes/DemoBasketLineAddUI/zcool_ceyyFont.ttf?fileID=12800000&guid=38eedc49de88a9c44826a16de993068a&type=3#zcool_ceyyFont');
