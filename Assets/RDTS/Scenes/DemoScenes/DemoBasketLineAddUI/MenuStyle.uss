    margin-left: 20px;
    margin-right: 20px;
    margin-top: 20px;
    margin-bottom: 20px;
    padding-left: 10px;
    padding-right: 10px;
    padding-top: 10px;
    padding-bottom: 10px;
    text-shadow: 0.5px 0.5px 0 rgb(0, 0, 0);
    color: rgba(255, 255, 255, 255);
    -unity-font-style: bold;
}

